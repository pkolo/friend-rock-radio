<div class="nested-fields">
  <%= f.input :title %>
  <%= f.input :year %>
  <div id="bands">
    <%= f.simple_fields_for :bands do |band| %>
      <%= render 'band_fields', f: band %>
    <% end %>
    <div class="links">
      <%= link_to_add_association 'add band', f, :bands %>
    </div>
  </div>
  <%= link_to_remove_association "remove track", f %>
</div>
